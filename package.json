{
  "name": "emohub",
  "type": "module",
  "packageManager": "bun@1.3.9",
  "workspaces": [
    "packages/shared",
    "packages/server",
    "apps/*"
  ],
  "scripts": {
    "dev:server": "bun --cwd packages/server run dev",
    "db:migrate": "bunx prisma migrate dev",
    "db:generate": "bunx prisma generate",
    "db:push": "bunx prisma db push",
    "typecheck": "bunx tsc --noEmit"
  },
  "devDependencies": {
    "@prisma/client": "6.19.2",
    "prisma": "6.19.2",
    "turbo": "^2.8.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "fastify": "^5.7.4",
    "fastify-plugin": "^5.1.0"
  }
}
